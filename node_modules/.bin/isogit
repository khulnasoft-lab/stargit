#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mdsulaiman/stargit/node_modules/.pnpm/isomorphic-git@1.30.2/node_modules/isomorphic-git/node_modules:/Users/mdsulaiman/stargit/node_modules/.pnpm/isomorphic-git@1.30.2/node_modules:/Users/mdsulaiman/stargit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mdsulaiman/stargit/node_modules/.pnpm/isomorphic-git@1.30.2/node_modules/isomorphic-git/node_modules:/Users/mdsulaiman/stargit/node_modules/.pnpm/isomorphic-git@1.30.2/node_modules:/Users/mdsulaiman/stargit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../isomorphic-git/cli.cjs" "$@"
else
  exec node  "$basedir/../isomorphic-git/cli.cjs" "$@"
fi
